{
  "name": "smart-polly",
  "homepage": "/",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "antd": "^5.8.4",
    "axios": "^1.6.0",
    "dayjs": "^1.11.9",
    "env-cmd": "^10.1.0",
    "html-react-parser": "^4.2.1",
    "lint-staged": "^14.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "5.0.1",
    "typescript": "^5.2.2",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start:local": "env-cmd -f src/env/local react-scripts start",
    "start:dev": "env-cmd -f src/env/dev react-scripts start",
    "start:test": "env-cmd -f src/env/test react-scripts start",
    "build:local": "env-cmd -f src/env/local react-scripts build",
    "build:dev": "env-cmd -f src/env/dev react-scripts build",
    "build:test": "env-cmd -f src/env/test react-scripts build",
    "build:prod": "GENERATE_SOURCEMAP=false env-cmd -f src/env/prod react-scripts build",
    "lint": "yarn lint-staged:js",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "eslint": "^8.48.0",
    "eslint-plugin-react": "^7.33.2",
    "mockjs": "^1.1.0"
  }
}
